<odoo>
    <record id="view_student_form" model="ir.ui.view">
		<field name="name">custom.student.form</field>
		<field name="model">custom.student</field>
		<field name="arch" type="xml"></field>
			<form>
				<sheet>
					<group>
						<field name="name"></field>
						<field name="surname"></field>
						<field name="email"></field>
						<field name="phone"></field>
						<field name="student_ids" widget="many2many_tags"></field>
					</group>
				</sheet>
			</form>
	</record>

	<record id="view_student_tree" model="ir.ui.view">
		<field name="name">custom.student.tree</field>
		<field name="model">custom.student</field>
		<field name="arch" type="xml">
			<tree>
				<field name="name"/>
				<field name="surname"/>
				<field name="email"/>
				<field name="phone"/>
			</tree>
		</field>
	</record>

	<menuitem id="menu_custom_students" name="Students" sequence="1" parent="base.menu_custom_root"/>
	 <menuitem id="submenu_students" name="Manage Students" parent="menu_custom_students" action="action_student_list"/>
</odoo>